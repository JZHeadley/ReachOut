<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="wrap_content"
    android:layout_width="fill_parent"
    android:orientation="vertical">


    <com.jzheadley.reachout.views.ThreeButtonView
        android:id="@+id/three_button_loan_amount"
        android:layout_height="match_parent"
        android:layout_width="wrap_content" />

    <com.jzheadley.reachout.views.ThreeButtonView
        android:id="@+id/three_button_repayment_amount"
        android:layout_height="match_parent"
        android:layout_width="wrap_content" />

    <com.jzheadley.reachout.views.ThreeButtonView
        android:id="@+id/three_button_repayment_date"
        android:layout_height="match_parent"
        android:layout_width="wrap_content" />

    <com.jzheadley.reachout.views.ThreeButtonView
        android:id="@+id/three_button_money_making"
        android:layout_height="match_parent"
        android:layout_width="wrap_content" />

    <com.jzheadley.reachout.views.ThreeButtonView
        android:id="@+id/three_button_loan_purchase"
        android:layout_height="match_parent"
        android:layout_width="wrap_content" />

    <com.jzheadley.reachout.views.ThreeButtonView
        android:id="@+id/three_button_how_money_help"
        android:layout_height="match_parent"
        android:layout_width="wrap_content" />
</LinearLayout>